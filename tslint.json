{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "trailing-comma": [ false ],
        "no-console": false,
        "max-classes-per-file": false
    },
    "variable-name": {
        "options": [
          "ban-keywords",
          "check-format",
          "allow-pascal-case",
          "allow-leading-underscore"
        ]
    },
    "rulesDirectory": []
}